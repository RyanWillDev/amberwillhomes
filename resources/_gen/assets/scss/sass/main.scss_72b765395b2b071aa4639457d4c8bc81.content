ul,ol{list-style:none;margin:0;padding:0}a{color:#000}body{font-family:-apple-system,BlinkMacSystemFont,segoe ui,Roboto,Helvetica,Arial,sans-serif,apple color emoji,segoe ui emoji,segoe ui symbol;padding:0;margin:0}body main{max-width:1600px;margin:0 auto}body main .bio{margin:0 auto;max-width:800px}.hero-banner{display:flex;position:relative;flex-wrap:wrap;justify-content:center;margin:0 auto 150px;width:100%}@media(min-width:700px){.hero-banner{margin-bottom:275px}}.hero-banner-image{position:relative;width:100%}@media(min-width:700px){.hero-banner-image{max-height:650px}}.hero-banner .photo-credit{bottom:5px;color:silver;font-size:.6rem;opacity:.7;position:absolute;right:5px}@media(min-width:700px){.hero-banner .photo-credit{font-size:.75rem}}.hero-banner .photo-credit a{color:inherit}.headshot-container{bottom:-125px;position:absolute}@media(min-width:700px){.headshot-container{bottom:-250px}}.headshot-container-image{border:solid 3px #f7c7f7;height:150px;width:150px;border-radius:50%}@media(min-width:700px){.headshot-container-image{height:300px;width:300px}}.icons{display:flex;flex-wrap:nowrap;justify-content:center}.icons .icon{height:20px;margin-right:16px;width:20px}@media(min-width:700px){.icons .icon{height:30px;margin-right:8px;width:30px}}.icons .icon a,.icons .icon svg{height:inherit;width:inherit}.icons .icon a>path,.icons .icon svg>path{fill:#f7c7f7}.icons .icon a{padding:5px}.footer{background-color:#ffff;display:flex;flex-wrap:wrap;justify-content:center;margin:0 auto;width:100%}@media(min-width:700px){.footer{justify-content:space-around}}.footer .lee-realty{display:flex;flex-wrap:nowrap;justify-content:center;width:100%}@media(min-width:700px){.footer .lee-realty{width:initial}}.footer .lee-realty img{margin-right:8px}.footer .lee-realty .info{display:flex;flex-wrap:wrap;font-size:.65rem;margin-top:24px}@media(min-width:700px){.footer .lee-realty .info{font-size:.75rem}}.footer .lee-realty .info span{display:block;margin:0;width:100%}.footer .junk .disclaimer{font-size:.5rem;text-align:center;margin-top:18px}@media(min-width:700px){.footer .junk .disclaimer{font-size:.65rem}}.footer .junk .logos{display:flex;justify-content:center}